{
  "name": "octopus",
  "displayName": "Octopus",
  "description": "Octopus input file language support with syntax highlighting, hover documentation, and documentation links",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/he0119/vscode-octopus.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "octopus",
        "aliases": [
          "Octopus",
          "octopus"
        ],
        "extensions": [
          ".inp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "octopus",
        "scopeName": "source.octopus",
        "path": "./syntaxes/octopus.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "octopus.showVariables",
        "title": "显示所有 Octopus 变量",
        "category": "Octopus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == octopus",
          "command": "octopus.showVariables",
          "group": "1_octopus"
        }
      ],
      "commandPalette": [
        {
          "command": "octopus.showVariables",
          "when": "resourceLangId == octopus"
        }
      ]
    }
  },
  "scripts": {
    "test": "node tests/test-suite.js",
    "test:verbose": "node tests/test-suite.js --verbose",
    "build": "node scripts/build.js",
    "parse-varinfo": "node scripts/parse-varinfo.js"
  },
  "devDependencies": {}
}
